$TIME_FACTOR = 1;
$start_at = 87 * $TIME_FACTOR;

@keyframes animationNormalFade {
  0% { opacity: 0; }
  12% { opacity: 1; }
  90% { opacity: 1; }
  100% { opacity: 0; }
}
/*
x = 35/100*7 + 87; window.player.seek(x)
document.querySelector('#text26').style.transform = 'rotate(-16deg) translate(-5.1%, -51.1%) scale(1.35)'
document.querySelector('#text26').style.animation = 'none'
document.querySelector('#text26').style.opacity = 1
*/
@keyframes logoAnimation {
  0% { // 87s
    opacity: 0;
    transform: rotate(-25deg) translate(-5.6%, -216%) scale(1.6);
  }
  15% {
    transform: rotate(-25deg) translate(-5.6%, -216%) scale(1.6);
  }
  18.5% {
    opacity: .5;
    transform: rotate(-25deg) translate(-6.2%, -204%) scale(1.6);;
  }
  19% {
    transform: rotate(-25deg) translate(-5.8%, -194%) scale(1.6);
  }
  20% {
    transform: rotate(-25deg) translate(-5.6%, -183%) scale(1.6);
  }
  22% {
    opacity: .75;
    transform: rotate(-20deg) translate(-7.1%, -141%) scale(1.6);
  }
  23% {
    transform: rotate(-20deg) translate(-8.1%, -124%) scale(1.5);
  }
  24% {
    transform: rotate(-20deg) translate(-6.6%, -109%) scale(1.5);
  }
  25% {
    opacity: 1;
    transform: rotate(-19.3deg) translate(-7.3%, -101.6%) scale(1.4);
  }
  26% {
    transform: rotate(-19.3deg) translate(-6.1%, -87.7%) scale(1.45);
  }
  30% {
    transform: rotate(-16deg) translate(-5.1%, -51.1%) scale(1.35);
  }
  35% {
    transform: rotate(-12deg) translate(-3%, -17.8%) scale(1.2);
  }
  40% {
    transform: rotate(-8deg) translate(-1.3%, -2.8%) scale(1.1);
  }
  50% {
    transform: rotate(-4deg) translate(0.5%, 1.4%) scale(1);
  }
  60% {
    transform: rotate(-2deg) translate(0.2%, 1.9%) scale(0.9);
  }
  70% {
    transform: rotate(-1deg) translate(1.7%, 1.2%) scale(0.9);
  }
  80% {
    transform: rotate(0deg) translate(3%, 1.7%) scale(0.9);
  }
  90% {
    transform: rotate(0deg) translate(3.5%, 2.1%) scale(0.9);
  }
  98% {
    transform: rotate(0deg) translate(2.7%, 1.7%) scale(0.85);

    // glow increase
  }
  99% {
    opacity: 1;
  }
  100% { // 94s
    opacity: 0;
    transform: rotate(0deg) translate(2.7%, 1.7%) scale(0.85);
  }
}

$strokeSize= 2px;
$strokeBlur= 2px;
$blackAlpha= #00000040;
$colorAlpha= #bf833940;
$whiteAlpha= #ffffff80;
$animationTextColor= #dcbc94;

#text26 {
  color: rgb(255, 243, 180);
  text-shadow: rgba(255, 255, 255, 0.43) 0px 0px 30px, rgb(196, 158, 58) 2px 2px 6px, rgb(196, 158, 58) 2px -2px 6px, rgb(196, 158, 58) -2px 2px 6px, rgb(196, 158, 58) -2px -2px 6px, rgb(160, 94, 29) 0px 0px 20px, rgb(230, 190, 53) -3px 0px 2px, rgb(230, 190, 53) 3px 0px 2px;
  letter-spacing: 0;
  font-size: 2.5em;
  font-family: GameOfThrones;

  // border: 2px solid red;
  // opacity: 1;

}

.animate {
  text-shadow:
    -1px -1px $strokeBlur $blackAlpha,
    -1px 0 $strokeBlur $blackAlpha,
    -1px 2px $strokeBlur $blackAlpha,
    0 2px $strokeBlur black,
    2px 2px $strokeBlur black,
    2px 0 $strokeBlur black,
    1px -1px $strokeBlur $blackAlpha,
    0 -1px $strokeBlur $blackAlpha,
    -1px -1px 2px $colorAlpha,
    -1px 0 2px $colorAlpha,
    0 -1px 2px $colorAlpha,
    -1px -1px 1px $whiteAlpha;
  color: $animationTextColor;
  white-space: pre-line;
  letter-spacing: .4em;
  line-height: 1.5;

  .video-overlay__content {
    display: flex;
    justify-content: center;
    align-items: center;

    > div {
      display: flex;
      align-items: center;
      justify-content: center;
      opacity: 0;
      animation-fill-mode: forwards;
      // background-color: rgba(255,255,255,0.2); // uncomment to show text box for development
    }
  }

  #text0  { animation: animationNormalFade $TIME_FACTOR * 2s linear $TIME_FACTOR * 4.4s - $start_at; }
  #text1  { animation: animationNormalFade $TIME_FACTOR * 2s linear $TIME_FACTOR * 6.7s - $start_at; }
  #text2  { animation: animationNormalFade $TIME_FACTOR * 2s linear $TIME_FACTOR * 9s - $start_at; }
